# platform :ios, '9.0'

def shared
    pod 'Swinject'
    pod 'SwinjectStoryboard'
    pod 'Fabric'
    pod 'Crashlytics'
    pod 'SwiftyJSON'
    pod 'Charts'

    # Firebase
    pod 'Firebase/Core'
end

target 'Crypto' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods

  shared	
end
